{
  "name": "yascriptlang",
  "version": "0.1.0",
  "description": "A simple scripting language inspired by Lisp",
  "main": "./bin/yascriptlang.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxbarsukov/yascriptlang.git"
  },
  "bugs": {
    "url": "https://github.com/maxbarsukov/yascriptlang/issues"
  },
  "author": {
    "name": "Max Barsukov",
    "email": "maximbarsukov@bk.ru"
  },
  "type": "module",
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.2",
    "husky": "^7.0.2",
    "prettier": "^2.3.2"
  },
  "scripts": {
    "lint": "eslint src/**/*.js",
    "format": "eslint src/**/*.js --fix",
    "prettier": "prettier --write src/**/*.js"
  },
  "bin": {
    "yascriptlang": "./bin/yascriptlang.js"
  },
  "preferGlobal": true,
  "engines": {
    "node": ">= 10.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "license": "MIT",
  "private": true
}
